{
	"compilerOptions": {
		"target": "es2022",
		"moduleDetection": "force",

		"module": "preserve",
		"resolveJsonModule": true,
		"allowJs": true,
		"strict": false,
		"noImplicitOverride": true,
		"noEmit": true,
		"esModuleInterop": true,
		"verbatimModuleSyntax": true,
		"skipLibCheck": true
	}
}
